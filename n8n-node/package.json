{
  "name": "n8n-nodes-whisper-transcription",
  "version": "1.0.0",
  "description": "N8N node for Whisper Transcription Service - Japanese-optimized video/audio transcription",
  "keywords": [
    "n8n-community-node-package",
    "whisper",
    "transcription",
    "speech-to-text",
    "japanese"
  ],
  "license": "MIT",
  "author": {
    "name": "VideoTranscriptAnalyzer"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "main": "index.js",
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint nodes credentials --ext .ts --fix",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/WhisperTranscriptionApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/WhisperTranscription/WhisperTranscription.node.js"
    ]
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "n8n-workflow": "^1.20.0",
    "typescript": "^5.3.0",
    "gulp": "^4.0.2"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  }
}
